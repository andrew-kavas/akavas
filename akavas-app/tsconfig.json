{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "lib": [
      "ESNext",
      "WebWorker",
      "DOM"
    ],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "noEmit": true,
    "skipLibCheck": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "target": "ESNext",
    "baseUrl": ".",
    "paths": {
      "#src/*": [
        "./src/*"
      ],
      "pave-shim/*": [
        "./pave-shim/*"
      ]
    },
    "types": [
      "vite/client"
    ]
  },
  "exclude": [
    "dist",
    "pave-shim/pave"
  ]
}